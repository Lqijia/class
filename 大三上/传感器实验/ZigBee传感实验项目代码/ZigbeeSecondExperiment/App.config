<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--串口-->
    <add key="ComStr" value="COM4"/>
    <!--波特率-->
    <add key="BaudRate" value="38400"/>
    <!--湿度Zigbee传感编号-->
    <add key="ZigbeeTemperatureSor" value="0001"/>
    <!--继电器Zigbee传感编号-->
    <add key="ZigbeeRelaySor" value="0002"/>
  </appSettings>
</configuration>